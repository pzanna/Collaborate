{
  "service": {
    "name": "SERVICE_NAME_PLACEHOLDER",
    "version": "1.0.0",
    "description": "SERVICE_DESCRIPTION_PLACEHOLDER",
    "host": "0.0.0.0",
    "port": 8000,
    "debug": false
  },
  "mcp": {
    "server_url": "ws://mcp-server:9000",
    "timeout": 30,
    "retry_attempts": 3,
    "retry_delay": 5
  },
  "database": {
    "url": "postgresql://postgres:password@postgres:5432/eunice",
    "pool_size": 10,
    "max_overflow": 20,
    "pool_timeout": 30
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_logging": false,
    "log_file": "/app/logs/SERVICE_NAME_PLACEHOLDER.log"
  },
  "health_check": {
    "enabled": true,
    "interval": 30,
    "timeout": 5,
    "endpoint": "/health"
  },
  "security": {
    "cors_enabled": true,
    "cors_origins": ["http://localhost:3000"],
    "rate_limiting": true,
    "max_requests_per_minute": 100
  },
  "capabilities": [
    "health_check",
    "metrics",
    "logging"
  ],
  "service_specific": {
    "comment": "Add service-specific configuration here"
  }
}
