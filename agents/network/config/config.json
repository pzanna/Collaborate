{
  "service": {
    "name": "network-agent",
    "type": "network",
    "version": "1.0.0",
    "host": "0.0.0.0",
    "port": 8004
  },
  "mcp": {
    "server_url": "ws://mcp-server:9000",
    "reconnect_attempts": 5,
    "reconnect_delay": 5,
    "ping_interval": 30,
    "ping_timeout": 10
  },
  "google_search": {
    "api_endpoint": "https://www.googleapis.com/customsearch/v1",
    "api_key": "",
    "search_engine_id": "",
    "default_page_size": 10,
    "max_results": 100,
    "timeout": 30,
    "rate_limit": {
      "max_requests_per_day": 100,
      "max_requests_per_minute": 10
    }
  },
  "capabilities": [
    "google_search",
    "web_search",
    "multi_page_search",
    "search_capabilities",
    "google_custom_search",
    "search_result_parsing"
  ],
  "logging": {
    "level": "DEBUG",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}
